package com.promineotech.plums.entity;

public enum Genre {

	// Keeping each element on its own line gives a sense of professionalism and organization
	// From online research, enums work with single word identifiers
	ADVENTURE("Adventure"),
	COMPUTER_FORENSICS("Computer Forensics"),
	CYBER_SECURITY("Cyber Security"), 
	EDUCATION("Education"), 
	FICTION("Fiction"), 
	JAVA("Java"), 
	MANAGEMENT("Management"), 
	RECIPES("Recipes"),
	SCIFI("Sci Fi"), 
	SPIRITUAL("Spiritual"), 
	THRILLER("Thriller");
		
	Genre(final String toString) {}

	
	
}
